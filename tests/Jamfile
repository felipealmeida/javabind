alias runtime-tests
 : [ run create-jvm.cpp /javabind//javabind /jvm//jvm ]
   [ run new_object.cpp /javabind//javabind /jvm//jvm : : NewObject NewObjectByte
          NewObjectChar NewObjectDouble NewObjectFloat NewObjectInt NewObjectLong
          NewObjectObject NewObjectShort ]
   [ run read_static_attribute.cpp /javabind//javabind /jvm//jvm : : 
          ReadStaticAttributeArrayBoolean ReadStaticAttributeArrayByte
          ReadStaticAttributeArrayChar ReadStaticAttributeArrayDouble
          ReadStaticAttributeArrayDouble ReadStaticAttributeArrayFloat
          ReadStaticAttributeArrayInt ReadStaticAttributeArrayLong
          ReadStaticAttributeArrayObject ReadStaticAttributeArrayShort
          ReadStaticAttributeBoolean ReadStaticAttributeByte
          ReadStaticAttributeChar ReadStaticAttributeDouble ReadStaticAttributeDouble
          ReadStaticAttributeFloat ReadStaticAttributeInt ReadStaticAttributeLong
          ReadStaticAttributeObject ReadStaticAttributeShort ]
   [ run read_attribute.cpp /javabind//javabind /jvm//jvm : : 
          ReadAttributeArrayBoolean ReadAttributeArrayByte ReadAttributeArrayChar
          ReadAttributeArrayDouble ReadAttributeArrayFloat ReadAttributeArrayInt
          ReadAttributeArrayLong ReadAttributeArrayObject ReadAttributeArrayShort
          ReadAttributeBoolean ReadAttributeByte ReadAttributeChar ReadAttributeDouble
          ReadAttributeFloat ReadAttributeInt ReadAttributeLong
          ReadAttributeObject ReadAttributeShort ]
   [ run call_method.cpp /javabind//javabind /jvm//jvm : : 
          CallMethodArrayBoolean CallMethodArrayByte CallMethodArrayChar
          CallMethodArrayDouble CallMethodArrayFloat CallMethodArrayInt
          CallMethodArrayLong CallMethodArrayObject CallMethodArrayShort
          CallMethodBoolean CallMethodByte
          CallMethodChar CallMethodDouble CallMethodFloat CallMethodInt
          CallMethodLong CallMethodObject CallMethodShort CallMethodVoid ]
   [ run call_static_method.cpp /javabind//javabind /jvm//jvm : : 
          CallStaticMethodArrayBoolean CallStaticMethodArrayByte CallStaticMethodArrayChar
          CallStaticMethodArrayDouble CallStaticMethodArrayFloat CallStaticMethodArrayInt
          CallStaticMethodArrayLong CallStaticMethodArrayObject CallStaticMethodArrayShort
          CallStaticMethodBoolean CallStaticMethodByte CallStaticMethodChar
          CallStaticMethodDouble CallStaticMethodFloat CallStaticMethodInt
          CallStaticMethodLong CallStaticMethodObject CallStaticMethodShort
          CallStaticMethodVoid ]
   [ run extends.cpp /javabind//javabind /jvm//jvm : : Extends ExtendsBase ]
   [ run implements.cpp /javabind//javabind /jvm//jvm : :
          Implements Interface1 Interface2 ]
   [ run signatures.cpp /javabind//javabind /jvm//jvm : : Signatures ]
 ;

alias tests : runtime-tests compile-tests ;

